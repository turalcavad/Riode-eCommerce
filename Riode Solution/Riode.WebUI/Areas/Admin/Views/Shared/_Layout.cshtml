	<![endif]-->
	<!-- Warning Section Ends -->
	<!-- Required Jquery -->
	<script type="text/javascript" src="~/admin/assets/bower_components/jquery/dist/jquery.min.js"></script>
	<script type="text/javascript" src="~/admin/assets/bower_components/jquery-ui/jquery-ui.min.js"></script>
	<script type="text/javascript" src="~/admin/assets/bower_components/popper.js/dist/umd/popper.min.js"></script>
	<script type="text/javascript" src="~/admin/assets/bower_components/bootstrap/dist/js/bootstrap.min.js"></script>
	<!-- jquery slimscroll js -->
	<script type="text/javascript" src="~/admin/assets/bower_components/jquery-slimscroll/jquery.slimscroll.js"></script>
	<!-- modernizr js -->
	<script type="text/javascript" src="~/admin/assets/bower_components/modernizr/modernizr.js"></script>
	<!-- Chart js -->
	<script type="text/javascript" src="~/admin/assets/bower_components/chart.js/dist/Chart.js"></script>
	<!-- amchart js -->
	<script src="~/admin/assets/pages/widget/amchart/amcharts.js"></script>
	<script src="~/admin/assets/pages/widget/amchart/serial.js"></script>
	<script src="~/admin/assets/pages/widget/amchart/light.js"></script>
	<script src="~/admin/assets/js/jquery.mCustomScrollbar.concat.min.js"></script>
	<script type="text/javascript" src="~/admin/assets/js/SmoothScroll.js"></script>
	<script src="~/admin/assets/js/pcoded.min.js"></script>
	<script src="~/admin/assets/libs/toastr.js/toastr.min.js"></script>
	<!-- custom js -->
	<script src="~/admin/assets/js/vartical-layout.min.js"></script>
	<script type="text/javascript" src="~/admin/assets/pages/dashboard/custom-dashboard.js"></script>
	<script type="text/javascript" src="~/admin/assets/js/script.min.js"></script>
	<script src="~/admin/assets/js/customize.js"></script>
	@RenderSection("js", required: false)
</body>



</html>